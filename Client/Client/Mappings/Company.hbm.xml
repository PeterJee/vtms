<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="true">
	<class name="VTMS.Model.Entities.Company,VTMS.Model.Entities" table="company" lazy="false">

		<!-- Primary Key(s) -->
		<id name="Id" column="id" type="String">
			<generator class="assigned"/>
		</id>
		
		<!-- Properties -->
		<property column="name" type="String" name="Name" not-null="true" length="45" />
		<property column="priority" type="Int32" name="Priority" not-null="true" />
		<property column="isactive" type="Boolean" name="Isactive" not-null="true" />
		
	</class>
</hibernate-mapping>
